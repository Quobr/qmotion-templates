{
  "name": "@qmotion/templates",
  "version": "0.1.0",
  "description": "Copy-paste templates for QMotion - shadcn/ui style components and scenes",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./registry": "./registry/index.json"
  },
  "files": [
    "dist",
    "registry",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "peerDependencies": {
    "@qmotion/core": "workspace:*",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quobr/qmotion.git",
    "directory": "packages/templates"
  },
  "keywords": [
    "qmotion",
    "templates",
    "video",
    "animation",
    "react"
  ],
  "license": "MIT"
}